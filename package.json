{
  "name": "sdsa",
  "version": "1.0.0",
  "main": "index.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "dev-client:ios": "eas build --profile development --platform ios",
    "dev-client:android": "eas build --profile development --platform android",
    "install:ci": "yarn install --frozen-lockfile",
    "install:dev": "yarn install",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "echo 'No tests configured yet'",
    "clean": "rm -rf node_modules .expo dist yarn-error.log",
    "clean:install": "yarn clean && yarn install",
    "prebuild": "expo prebuild",
    "postinstall": "echo 'Dependencies installed successfully'",
    "verify": "node scripts/verify-setup.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@google/generative-ai": "^0.24.1",
    "@react-native-async-storage/async-storage": "2.1.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "expo": "~53.0.22",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-safe-area-context": "4.14.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "^0.20.0",
    "yaml": "^2.6.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-expo": "^9.2.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3"
  },
  "private": true
}
