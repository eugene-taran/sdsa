# Environment Variables Template
# Copy this file to .env and fill in your values

# Node Environment
NODE_ENV=development

# Expo Configuration
EXPO_PUBLIC_API_URL=https://api.sdsa.team
EXPO_PUBLIC_KNOWLEDGE_BASE_URL=https://raw.githubusercontent.com/eugene-taran/sdsa.team/main

# Firebase Configuration (for future use)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=

# RevenueCat Configuration (for future use)
EXPO_PUBLIC_REVENUECAT_API_KEY=

# Gemini API Configuration
# Get your API key from: https://makersuite.google.com/app/apikey
EXPO_PUBLIC_GEMINI_API_KEY=

# Optional: Gemini Model Configuration
# EXPO_PUBLIC_GEMINI_JOKE_MODEL=gemini-2.5-flash-image-preview
# EXPO_PUBLIC_GEMINI_FLASH_MODEL=gemini-2.5-flash

# Model Download URLs (for future use with Rock)
EXPO_PUBLIC_PHI3_MODEL_URL=
EXPO_PUBLIC_LLAMA_MODEL_URL=

# Feature Flags
EXPO_PUBLIC_ENABLE_MOCK_DATA=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
EXPO_PUBLIC_ENABLE_DEV_TOOLS=true